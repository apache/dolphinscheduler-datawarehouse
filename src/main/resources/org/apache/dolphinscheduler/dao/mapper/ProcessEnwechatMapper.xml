<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.apache.dolphinscheduler.dao.mapper.ProcessEnwechatMapper">
    <insert id="batchInsert" parameterType="java.util.List" useGeneratedKeys="true" keyColumn="id" keyProperty="id">
        insert into  `bz_zr_process_enwechat`
        (process_id,enterprise_wechat_account)
        values 
        <foreach collection="list" item="item" separator=",">
            (
             #{item.processId},#{item.enterpriseWechatAccount}
            )
        </foreach>
    </insert>

<!--    <delete id="deleteProcessEnwechats" parameterType="int">-->
    <delete id="deleteProcessEnwechats" >
        delete from bz_zr_process_enwechat where process_id = #{processId}
    </delete>


    <select id="selectEnwechatsByProcessId" resultType="java.lang.String">
        select enterprise_wechat_account
        from bz_zr_process_enwechat
        where process_id = #{processId}
    </select>


</mapper>