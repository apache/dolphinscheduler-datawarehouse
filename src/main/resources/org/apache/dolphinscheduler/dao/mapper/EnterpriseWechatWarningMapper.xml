<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.apache.dolphinscheduler.dao.mapper.EnterpriseWechatWarningMapper">
    <insert id="batchInsert" parameterType="java.util.List" useGeneratedKeys="true" keyColumn="warning_id" keyProperty="warningId">
        insert into  `bz_zr_warning_enterprise_wechat`
        (enterprise_wechat_account)
        values 
        <foreach collection="list" item="item" separator=",">
            (
             #{item.enterpriseWechatAccount}
            )
        </foreach>
    </insert>

    <delete id="deleteByWarningIds" parameterType="java.util.List">
        <if test = "ids != null and !ids.isEmpty()">
            delete from bz_zr_warning_enterprise_wechat where warning_id in
            <foreach collection="ids" item="item" open="(" close=")" separator=",">
                #{item}
            </foreach>
        </if>
    </delete>


    <select id="selectEnWechatsByIds" resultType="java.lang.String">
        select enterprise_wechat_account
        from bz_zr_warning_enterprise_wechat
        where warning_id in
        <foreach collection="ids" item="item" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>


</mapper>